<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ProjectGuid>{B7A7DF45-4248-4784-8417-594F15B7484A}</ProjectGuid>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>netcoreapp3.1</TargetFrameworks>
    <AssemblyTitle>PullRequestReleaseNotes</AssemblyTitle>
    <Company>Jasmin Sehic</Company>
    <Product>PullRequestReleaseNotes</Product>
    <Description>Builds release history notes from unreleased pull requests on GitHub, GitLab, Azure DevOps Services / Server, BitBucket Cloud and Bitbucket Server and optionally posts them to a markdown file, Atlassian Confluence and Slack.</Description>
    <Copyright>Copyright © Jasmin Sehic 2019</Copyright>
    <AssemblyVersion>1.2.5.0</AssemblyVersion>
    <FileVersion>1.2.5.0</FileVersion>
    <Version>1.2.5</Version>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <RestorePackagesPath>$(SolutionDir)\packages</RestorePackagesPath>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <PackageProjectUrl>https://github.com/jasminsehic/PullRequestReleaseNotes</PackageProjectUrl>
    <RepositoryUrl>https://github.com/jasminsehic/PullRequestReleaseNotes</RepositoryUrl>
    <PackageTags>git pullrequestreleasenotes pullrequest github gitlab bitbucket tfs teamservices azure devops history notes releasenotes changelog gitversion atlassian confluence slack markdown</PackageTags>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>dotnet-pullrequestreleasenotes</ToolCommandName>
    <!--https://github.com/libgit2/libgit2sharp/issues/1754-->
    <IncludeSymbolsInSingleFile>true</IncludeSymbolsInSingleFile>
    <PackageId>PullRequestReleaseNotes.DotNetCore</PackageId>
    <Authors>Jasmin Sehic</Authors>
    <Owners>Jasmin Sehic</Owners>
    <Title>PullRequestReleaseNotes.DotNetCore</Title>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageIcon>logo.png</PackageIcon>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Infinity.NET" Version="0.2.1" />
    <PackageReference Include="LibGit2Sharp" Version="0.26.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="Octokit" Version="0.32.0" />
    <PackageReference Include="PowerArgs" Version="3.6.0" />
    <PackageReference Include="RestSharp" Version="106.6.10" />
    <PackageReference Include="SharpBucket" Version="0.11.0" />
    <PackageReference Include="YamlDotNet" Version="6.1.2" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\docs\img\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>